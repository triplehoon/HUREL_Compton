<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:conv="clr-namespace:HUREL_Imager_GUI.Converters">

    <conv:BoolToStringConverter x:Key="ConvStartOrStop" FalseValue="Start Session" TrueValue="Stop Session"/>
    <conv:BoolToBrushConverter x:Key="ConvBoolToGreenOrRed" TrueValue="Green" FalseValue="Red"/>
    <conv:TimespaneToStringConverter x:Key="ConvTimeSpanToStr"/>
    <conv:BoolToVisibilityConverter x:Key="ConvBoolToVisibility"  TrueValue="Visible" FalseValue="Collapsed"/>
    <conv:BoolToVisibilityConverter x:Key="ConvBoolToVisibilityReverse"  TrueValue="Collapsed" FalseValue="Visible"/>
    <conv:BooleanToReverseConverter x:Key="BooleanToReverseConverter"/>

</ResourceDictionary>